<!DOCTYPE html>
<html>
  <head>
    <title>Smartmallapp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    "//maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" %>
    <%= stylesheet_link_tag    "//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css" %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag "https://code.jquery.com/ui/1.12.1/jquery-ui.js" %>
    <script>
        $( function() {
            $( "#datepicker" ).datepicker();
        } );
    </script>
  </head>

  <body>
  <%= render 'layouts/nav' %>
    <%= yield %>
  </body>
</html>
